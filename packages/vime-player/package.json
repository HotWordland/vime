{
  "name": "@vime/player",
  "description": "Vime Player",
  "version": "1.0.0",
  "svelte": "src/index.js",
  "main": "dist/player.js",
  "module": "dist/player.esm.js",
  "unpkg": "dist/player.js",
  "jsdelivr": "dist/player.js",
  "sideEffects": false,
  "license": "MIT",
  "author": "Rahim Alwer <rahim_alwer@hotmail.com>",
  "homepage": "https://github.com/vime-js/vime#readme",
  "repository": "https://github.com/vime-js/vime/tree/master/packages/vime-player",
  "bugs": {
    "url": "https://github.com/vime-js/vime/issues"
  },
  "keywords": [
    "vime",
    "player",
    "youtube",
    "dailymotion",
    "vimeo",
    "html5",
    "lazy",
    "lightweight",
    "fast",
    "embed"
  ],
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c --environment NODE_ENV:production",
    "build:dev": "rollup -c --environment NODE_ENV:development",
    "svg:min": "svgo --config ./svgo.yml -f src/static/icons",
    "svg:sprite": "svgstore --inline -o src/static/vime.svg src/static/icons/*.svg"
  },
  "dependencies": {
    "@vime/core": "^1.0.0",
    "@vime/utils": "^1.0.0",
    "@vime/html5": "^1.0.0",
    "@vime/youtube": "^1.0.0",
    "@vime/vimeo": "^1.0.0",
    "@vime/dailymotion": "^1.0.0",
    "svgo": "^1.3.2",
    "svgstore-cli": "^1.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}